{
  "name": "vaishnav-v",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn sitemap",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./src/scripts/generateSitemap.mjs",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mdx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx,css,json}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mdx,css,json}\"",
    "check": "yarn format:check && yarn lint && yarn type-check",
    "check:fix": "yarn format:fix && yarn lint:fix",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/forms": "^0.5.11",
    "@vercel/analytics": "^1.3.1",
    "formik": "^2.4.5",
    "framer-motion": "^12.27.5",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.0.7",
    "next": "^16.1.4",
    "next-seo": "^6.8.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.4",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.19.30",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globby": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.12",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}